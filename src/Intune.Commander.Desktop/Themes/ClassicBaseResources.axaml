<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--
        Fluent-compatibility shim for Classic theme.

        All SystemControl* keys here are defined as GLOBAL resources (not inside
        ThemeDictionaries) because the Classic theme's own compiled XAML control
        templates reference many of them as StaticResource, which requires them to
        exist in the style tree at AXAML load time.

        Windows Classic palette used for semantic mapping:
          Face     #C0C0C0   (button / panel background)
          Shadow   #808080   (border shadow / disabled)
          Highlight #FFFFFF  (bright edge)
          Text     #000000   (default text)
          Accent   #000080   (selection / focus navy)
    -->
    <Styles.Resources>

        <!-- ── The one key that is defined GLOBALLY in Fluent (not per-theme) ── -->
        <SolidColorBrush x:Key="SystemControlTransparentBrush" Color="Transparent" />

        <!-- ── Transparent / reveal variants (all transparent) ── -->
        <SolidColorBrush x:Key="SystemControlDisabledTransparentBrush"                 Color="Transparent" />
        <SolidColorBrush x:Key="SystemControlForegroundTransparentBrush"               Color="Transparent" />
        <SolidColorBrush x:Key="SystemControlHighlightTransparentBrush"                Color="Transparent" />
        <SolidColorBrush x:Key="SystemControlHighlightAltTransparentBrush"             Color="Transparent" />
        <SolidColorBrush x:Key="SystemControlHighlightAltTransparentRevealBorderBrush" Color="Transparent" />
        <SolidColorBrush x:Key="SystemControlBackgroundTransparentRevealBorderBrush"   Color="Transparent" />
        <SolidColorBrush x:Key="SystemControlTransparentRevealBackgroundBrush"         Color="Transparent" />
        <SolidColorBrush x:Key="SystemControlTransparentRevealBorderBrush"             Color="Transparent" />
        <SolidColorBrush x:Key="SystemControlPageBackgroundTransparentBrush"           Color="Transparent" />

        <!-- ── Background brushes ── -->
        <SolidColorBrush x:Key="SystemControlBackgroundChromeMediumBrush"         Color="#C0C0C0" />
        <SolidColorBrush x:Key="SystemControlBackgroundChromeMediumLowBrush"      Color="#D4D0C8" />
        <SolidColorBrush x:Key="SystemControlBackgroundAltHighBrush"              Color="#FFFFFF" />
        <SolidColorBrush x:Key="SystemControlBackgroundAltMediumHighBrush"        Color="#F0F0F0" />
        <SolidColorBrush x:Key="SystemControlBackgroundAltMediumBrush"            Color="#E8E8E8" />
        <SolidColorBrush x:Key="SystemControlBackgroundAltMediumLowBrush"         Color="#DFDFDF" />
        <SolidColorBrush x:Key="SystemControlBackgroundBaseHighBrush"             Color="#000000" />
        <SolidColorBrush x:Key="SystemControlBackgroundBaseLowBrush"              Color="#1A000000" />
        <SolidColorBrush x:Key="SystemControlBackgroundBaseMediumBrush"           Color="#99000000" />
        <SolidColorBrush x:Key="SystemControlBackgroundBaseMediumHighBrush"       Color="#CC000000" />
        <SolidColorBrush x:Key="SystemControlBackgroundBaseMediumLowBrush"        Color="#66000000" />
        <SolidColorBrush x:Key="SystemControlBackgroundChromeBlackHighBrush"      Color="#FF000000" />
        <SolidColorBrush x:Key="SystemControlBackgroundChromeBlackMediumBrush"    Color="#CC000000" />
        <SolidColorBrush x:Key="SystemControlBackgroundChromeBlackLowBrush"       Color="#33000000" />
        <SolidColorBrush x:Key="SystemControlBackgroundChromeBlackMediumLowBrush" Color="#66000000" />
        <SolidColorBrush x:Key="SystemControlBackgroundChromeWhiteBrush"          Color="#FFFFFF" />
        <SolidColorBrush x:Key="SystemControlBackgroundListLowBrush"              Color="#19000000" />
        <SolidColorBrush x:Key="SystemControlBackgroundListMediumBrush"           Color="#33000000" />
        <SolidColorBrush x:Key="SystemControlBackgroundAccentBrush"               Color="#000080" />
        <SolidColorBrush x:Key="SystemControlBackgroundChromeWhiteRevealBorderBrush" Color="#FFFFFF" />

        <!-- ── Foreground brushes ── -->
        <SolidColorBrush x:Key="SystemControlForegroundBaseHighBrush"             Color="#000000" />
        <SolidColorBrush x:Key="SystemControlForegroundBaseLowBrush"              Color="#1A000000" />
        <SolidColorBrush x:Key="SystemControlForegroundBaseMediumBrush"           Color="#99000000" />
        <SolidColorBrush x:Key="SystemControlForegroundBaseMediumHighBrush"       Color="#CC000000" />
        <SolidColorBrush x:Key="SystemControlForegroundBaseMediumLowBrush"        Color="#808080" />
        <SolidColorBrush x:Key="SystemControlForegroundAltHighBrush"              Color="#FFFFFF" />
        <SolidColorBrush x:Key="SystemControlForegroundAltMediumHighBrush"        Color="#E0E0E0" />
        <SolidColorBrush x:Key="SystemControlForegroundAccentBrush"               Color="#000080" />
        <SolidColorBrush x:Key="SystemControlForegroundChromeBlackHighBrush"      Color="#000000" />
        <SolidColorBrush x:Key="SystemControlForegroundChromeBlackMediumBrush"    Color="#CC000000" />
        <SolidColorBrush x:Key="SystemControlForegroundChromeBlackMediumLowBrush" Color="#66000000" />
        <SolidColorBrush x:Key="SystemControlForegroundChromeHighBrush"           Color="#767676" />
        <SolidColorBrush x:Key="SystemControlForegroundChromeMediumBrush"         Color="#C0C0C0" />
        <SolidColorBrush x:Key="SystemControlForegroundChromeWhiteBrush"          Color="#FFFFFF" />
        <SolidColorBrush x:Key="SystemControlForegroundChromeDisabledLowBrush"    Color="#ABABAB" />
        <SolidColorBrush x:Key="SystemControlForegroundChromeGrayBrush"           Color="#767676" />
        <SolidColorBrush x:Key="SystemControlForegroundListLowBrush"              Color="#19000000" />
        <SolidColorBrush x:Key="SystemControlForegroundListMediumBrush"           Color="#33000000" />

        <!-- ── Disabled brushes ── -->
        <SolidColorBrush x:Key="SystemControlDisabledAccentBrush"              Color="#808080" />
        <SolidColorBrush x:Key="SystemControlDisabledBaseHighBrush"            Color="#000000" />
        <SolidColorBrush x:Key="SystemControlDisabledBaseLowBrush"             Color="#1A000000" />
        <SolidColorBrush x:Key="SystemControlDisabledBaseMediumLowBrush"       Color="#808080" />
        <SolidColorBrush x:Key="SystemControlDisabledChromeDisabledHighBrush"  Color="#ABABAB" />
        <SolidColorBrush x:Key="SystemControlDisabledChromeDisabledLowBrush"   Color="#CCCCCC" />
        <SolidColorBrush x:Key="SystemControlDisabledChromeHighBrush"          Color="#767676" />
        <SolidColorBrush x:Key="SystemControlDisabledChromeMediumLowBrush"     Color="#D4D0C8" />
        <SolidColorBrush x:Key="SystemControlDisabledListMediumBrush"          Color="#33000000" />

        <!-- ── Highlight brushes ── -->
        <SolidColorBrush x:Key="SystemControlHighlightAccentBrush"                      Color="#000080" />
        <SolidColorBrush x:Key="SystemControlHighlightAltAccentBrush"                   Color="#000080" />
        <SolidColorBrush x:Key="SystemControlHighlightAltAltHighBrush"                  Color="#FFFFFF" />
        <SolidColorBrush x:Key="SystemControlHighlightAltBaseHighBrush"                 Color="#000000" />
        <SolidColorBrush x:Key="SystemControlHighlightAltBaseLowBrush"                  Color="#1A000000" />
        <SolidColorBrush x:Key="SystemControlHighlightAltBaseMediumBrush"               Color="#99000000" />
        <SolidColorBrush x:Key="SystemControlHighlightAltBaseMediumHighBrush"           Color="#CC000000" />
        <SolidColorBrush x:Key="SystemControlHighlightAltAltMediumHighBrush"            Color="#E0E0E0" />
        <SolidColorBrush x:Key="SystemControlHighlightAltBaseMediumLowBrush"            Color="#66000000" />
        <SolidColorBrush x:Key="SystemControlHighlightAltListAccentHighBrush"           Color="#000080" Opacity="0.7" />
        <SolidColorBrush x:Key="SystemControlHighlightAltListAccentLowBrush"            Color="#000080" Opacity="0.4" />
        <SolidColorBrush x:Key="SystemControlHighlightAltListAccentMediumBrush"         Color="#000080" Opacity="0.6" />
        <SolidColorBrush x:Key="SystemControlHighlightAltChromeWhiteBrush"              Color="#FFFFFF" />
        <SolidColorBrush x:Key="SystemControlHighlightBaseHighBrush"                    Color="#000000" />
        <SolidColorBrush x:Key="SystemControlHighlightBaseLowBrush"                     Color="#1A000000" />
        <SolidColorBrush x:Key="SystemControlHighlightBaseMediumBrush"                  Color="#99000000" />
        <SolidColorBrush x:Key="SystemControlHighlightBaseMediumHighBrush"              Color="#CC000000" />
        <SolidColorBrush x:Key="SystemControlHighlightBaseMediumLowBrush"               Color="#66000000" />
        <SolidColorBrush x:Key="SystemControlHighlightChromeAltLowBrush"                Color="#D4D0C8" />
        <SolidColorBrush x:Key="SystemControlHighlightChromeHighBrush"                  Color="#767676" />
        <SolidColorBrush x:Key="SystemControlHighlightChromeWhiteBrush"                 Color="#FFFFFF" />
        <SolidColorBrush x:Key="SystemControlHighlightListAccentHighBrush"              Color="#000080" Opacity="0.7" />
        <SolidColorBrush x:Key="SystemControlHighlightListAccentLowBrush"               Color="#000080" Opacity="0.4" />
        <SolidColorBrush x:Key="SystemControlHighlightListAccentMediumBrush"            Color="#000080" Opacity="0.6" />
        <SolidColorBrush x:Key="SystemControlHighlightListMediumBrush"                  Color="#33000000" />
        <SolidColorBrush x:Key="SystemControlHighlightListLowBrush"                     Color="#19000000" />
        <SolidColorBrush x:Key="SystemControlHighlightListLowRevealBackgroundBrush"     Color="#33000000" />
        <SolidColorBrush x:Key="SystemControlHighlightListMediumRevealBackgroundBrush"  Color="#33000000" />
        <SolidColorBrush x:Key="SystemControlHighlightAccentRevealBackgroundBrush"      Color="#000080" />
        <SolidColorBrush x:Key="SystemControlHighlightAccent2RevealBackgroundBrush"     Color="#000080" Opacity="0.8" />
        <SolidColorBrush x:Key="SystemControlHighlightAccent3RevealBackgroundBrush"     Color="#000080" Opacity="0.6" />

        <!-- ── Focus visual brushes ── -->
        <SolidColorBrush x:Key="SystemControlFocusVisualPrimaryBrush"   Color="#000000" />
        <SolidColorBrush x:Key="SystemControlFocusVisualSecondaryBrush" Color="#FFFFFF" />
        <SolidColorBrush x:Key="SystemControlRevealFocusVisualBrush"    Color="#000080" />

        <!-- ── Hyperlink brushes ── -->
        <SolidColorBrush x:Key="SystemControlHyperlinkTextBrush"           Color="#000080" />
        <SolidColorBrush x:Key="SystemControlHyperlinkBaseHighBrush"       Color="#000000" />
        <SolidColorBrush x:Key="SystemControlHyperlinkBaseMediumBrush"     Color="#99000000" />
        <SolidColorBrush x:Key="SystemControlHyperlinkBaseMediumHighBrush" Color="#CC000000" />

        <!-- ── Page background / text brushes ── -->
        <SolidColorBrush x:Key="SystemControlPageBackgroundAltHighBrush"            Color="#FFFFFF" />
        <SolidColorBrush x:Key="SystemControlPageBackgroundAltMediumBrush"          Color="#E8E8E8" />
        <SolidColorBrush x:Key="SystemControlPageBackgroundMediumAltMediumBrush"    Color="#E8E8E8" />
        <SolidColorBrush x:Key="SystemControlPageBackgroundBaseLowBrush"            Color="#1A000000" />
        <SolidColorBrush x:Key="SystemControlPageBackgroundBaseMediumBrush"         Color="#99000000" />
        <SolidColorBrush x:Key="SystemControlPageBackgroundListLowBrush"            Color="#19000000" />
        <SolidColorBrush x:Key="SystemControlPageBackgroundChromeLowBrush"          Color="#F0F0F0" />
        <SolidColorBrush x:Key="SystemControlPageBackgroundChromeMediumLowBrush"    Color="#D4D0C8" />
        <SolidColorBrush x:Key="SystemControlPageTextBaseHighBrush"                 Color="#000000" />
        <SolidColorBrush x:Key="SystemControlPageTextBaseMediumBrush"               Color="#808080" />
        <SolidColorBrush x:Key="SystemControlPageTextChromeBlackMediumLowBrush"     Color="#66000000" />
        <SolidColorBrush x:Key="SystemControlDescriptionTextForegroundBrush"        Color="#808080" />

        <!-- ── Error / transient brushes ── -->
        <SolidColorBrush x:Key="SystemControlErrorTextForegroundBrush" Color="#CC0000" />
        <SolidColorBrush x:Key="SystemControlTransientBorderBrush"     Color="#000000" Opacity="0.14" />
        <SolidColorBrush x:Key="SystemControlTransientBackgroundBrush" Color="#D4D0C8" />

        <!-- ── Accent color ── -->
        <Color x:Key="SystemAccentColor">#000080</Color>

    </Styles.Resources>
</Styles>
